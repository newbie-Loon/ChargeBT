<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wellcharge"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="33" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />

    <permission
        android:name="com.example.wellcharge.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.wellcharge.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WellCharge" >
        <activity
            android:name="com.example.wellcharge.SleepModeActivity"
            android:exported="false"
            android:label="@string/title_activity_sleep_mode"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.LedSettingActivity"
            android:exported="false"
            android:label="@string/title_activity_led_setting"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.SoundSettingActivity"
            android:exported="false"
            android:label="@string/title_activity_sound_setting"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.TimeModeSettingActivity"
            android:exported="false"
            android:label="@string/title_activity_time_mode_setting"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.TempSettingActivity"
            android:exported="false"
            android:label="@string/title_activity_temp_setting"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.ModeSettingActivity"
            android:exported="false"
            android:label="@string/title_activity_mode_setting"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.EtcSettingActivity"
            android:exported="false"
            android:label="@string/title_activity_etc_setting"
            android:theme="@style/Theme.WellCharge" />

        <service
            android:name="com.example.wellcharge.service.BTKeepConnService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name="com.example.wellcharge.SetVoltageAvtivity"
            android:exported="false"
            android:label="@string/title_activity_set_voltage"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.SetTimePickerActivity"
            android:exported="false"
            android:label="@string/title_activity_set_time_picker"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.MainDisplayActivity"
            android:exported="false"
            android:label="@string/title_activity_main_display"
            android:theme="@style/Theme.WellCharge" />
        <activity
            android:name="com.example.wellcharge.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.WellCharge" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- <activity -->
        <!-- android:name=".MainActivity" -->
        <!-- android:exported="true" -->
        <!-- android:label="@string/app_name" -->
        <!-- android:theme="@style/Theme.WellCharge"> -->
        <!-- <intent-filter> -->
        <!-- <action android:name="android.intent.action.MAIN" /> -->
        <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
        <!-- </intent-filter> -->
        <!-- </activity> -->
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="androidx.activity.ComponentActivity"
            android:exported="true" />
        <activity
            android:name="androidx.compose.ui.tooling.PreviewActivity"
            android:exported="true" />

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.wellcharge.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <uses-library
            android:name="androidx.window.extensions"
            android:required="false" />
        <uses-library
            android:name="androidx.window.sidecar"
            android:required="false" />

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>