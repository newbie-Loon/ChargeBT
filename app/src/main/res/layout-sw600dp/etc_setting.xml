<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/artboard_3">

    <Button
        android:id="@+id/sleepSetting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/asset_4"
        android:fontFamily="@font/sriracha"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.684"
        app:layout_constraintWidth_percent="0.7"
        app:layout_constraintHeight_percent="0.085"/>

    <Button
        android:id="@+id/modeSetting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/asset_4"
        android:fontFamily="@font/sriracha"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.119"
        app:layout_constraintWidth_percent="0.7"
        app:layout_constraintHeight_percent="0.085"/>

    <Button
        android:id="@+id/ledSetting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/asset_4"
        android:fontFamily="@font/sriracha"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.539"
        app:layout_constraintWidth_percent="0.7"
        app:layout_constraintHeight_percent="0.085"/>

    <Button
        android:id="@+id/soundSetting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/asset_4"
        android:fontFamily="@font/sriracha"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4"
        app:layout_constraintWidth_percent="0.7"
        app:layout_constraintHeight_percent="0.085"/>

    <Button
        android:id="@+id/tempSetting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/asset_4"
        android:fontFamily="@font/sriracha"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="@dimen/_20sdp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.258"
        app:layout_constraintWidth_percent="0.7"
        app:layout_constraintHeight_percent="0.085"/>

    <Button
        android:id="@+id/backBtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/asset_18"
        android:fontFamily="@font/sriracha"
        android:textSize="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.865"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintHeight_percent="0.1"/>


</androidx.constraintlayout.widget.ConstraintLayout>